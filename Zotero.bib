	pages = {4572--4577}
}

@inproceedings{asada_data-driven_2016,
	title = {A data-driven approach to precise linearization of nonlinear dynamical systems in augmented latent space},
	doi = {10.1109/ACC.2016.7525186},
	abstract = {A new approach to linearize a complex nonlinear dynamical system using latent variables is presented in this paper. It is shown that a class of nonlinear dynamical systems behaves linearly when it is recast in a high-dimensional latent variable space. Independent state variables are augmented by adding auxiliary variables that are needed for describing constitutive laws of individual system components, which may be nonlinear. The augmented space is then reduced to a minimum dimensional space through latent variable analysis of the data created with simulation of the original nonlinear dynamic equations. Based on Bond-Graph model of a dynamical system, the minimum-order of the augmented latent space that can completely recover all the original data points is obtained. A method for further truncating the latent variables for real-time computation is also obtained. Numerical simulations demonstrate that the proposed method can produce the precise linearization of a nonlinear system, strikingly different from the standard linear approximation using Taylor expansion.},
	booktitle = {2016 {American} {Control} {Conference} ({ACC})},
	author = {Asada, H. H. and Wu, F. and Girard, A. and Mayalu, M.},
	month = jul,
	year = {2016},
	keywords = {Covariance matrices, Data models, Junctions, Linear approximation, bond-graph model, data-driven approach, graph theory, latent variable space, linearisation techniques, mathematical model, nonlinear dynamic equation, nonlinear dynamical system linearization, nonlinear dynamical systems, nonlinear equations},
	pages = {1838--1844}
}

@inproceedings{fumagalli_mvsa-ut:_2012,
	title = {The {mVSA}-{UT}: {A} miniaturized differential mechanism for a continuous rotational variable stiffness actuator},
