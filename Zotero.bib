@inproceedings{duval_flexsea-execute:_2016,
	title = {{FlexSEA}-{Execute}: {Advanced} motion controller for wearable robotic applications},
	shorttitle = {{FlexSEA}-{Execute}},
	doi = {10.1109/BIOROB.2016.7523771},
	abstract = {Wearable robots, such as powered prostheses and active exoskeletons, often rely on electric motors for actuation. Emulating biological joint angle and torque profiles requires special sensors, high peak power and advanced real-time controls. Safety is paramount and fail-safe circuits are required to detect and correct problematic situations. A smaller, lighter circuit can lead to a more efficient and affordable robot. Few commercial motor drivers accommodate all of these requirements. In this paper we present FlexSEA-Execute (Figure 1), the advanced motion controller part of FlexSEA, the FLEXible, Scalable Electronics Architecture designed for wearable robotic applications. At 36cm3 and 34.8g, this PSoC-based design integrates a 8A/25A (continuous/pulse) brushed/brushless motor driver, a safety co-processor, multi-drop RS-485, a strain gauge amplifier, a 6-axis inertial motion unit (IMU), USB, and a programmable expansion connector.},
	booktitle = {{IEEE} {Int}. {Conf}. on {Biomedical} {Robotics} and {Biomechatronics} ({BioRob})},
	author = {Duval, J. F. and Herr, H. M.},
	month = jun,
	year = {2016},
	keywords = {6-axis inertial motion unit, Brushless motors, Connectors, FlexSEA-Execute, IMU, Medical robotics, Microcontrollers, PSoC-based design, Robot sensing systems, Safety, Sensors, USB, active exoskeletons, actuation, biological joint angle, brushless machines, brushless motor, continuous/pulse brushed/brushless motor driver, control, coprocessors, electric motors, embedded system, fail-safe circuits, flexible scalable electronics architecture, motion control, motion controller, motor drivers, motor drives, multidrop RS-485, power electronics, powered prostheses, programmable expansion connector, real-time controls, safety coprocessor, strain gauge amplifier, system-on-chip, torque profiles, wearable computers, wearable robotic applications, wearable robotics},
	pages = {1056--1061}
}

@inproceedings{chen_generalized_1991,
	title = {The generalized principle of inertia match for geared robotic mechanisms},
	doi = {10.1109/ROBOT.1991.131788},
	abstract = {The principle of inertia match is extended from one-degree-of-freedom (DOF) systems to multi-DOF systems. Based on the concept of maximum acceleration capacity, a methodology for the determination of gear ratios in geared robotic mechanisms is developed. It is found that, in the optimum design, the mass inertia matrix of the input links reflected at the joint-space is equal to that of the major links, and the maximum acceleration capacity is independent of the gear train arrangement. Several two-DOF geared robotic mechanisms are used as design examples to illustrate the principle. Using this methodology, mechanisms can be designed to yield optimum dynamic performance},
	booktitle = {{IEEE} {Int}. {Conf}. on {Robotics} and {Automation} ({ICRA})},
	author = {Chen, D. Z. and Tsai, L. W.},
	month = apr,
	year = {1991},
