	keywords = {Automatic transmission, DC motors, Sun, Torque, actuator system, actuators, automobile, dual-motor system, dual-motors, energy efficiency enhancement, gears, mobile robot, mobile robot applications, mobile robots, planetary gear, speed-torque performance, speed–torque characteristic},
